	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"build": "npx tailwindcss build src/tailwind-source.css -o src/tailwind.min.css && tsc && cp src/tailwind.min.css dist/",
		"watch": "concurrently \"npm:watch:tsc\" \"npm:watch:tailwind\" \"npm:watch:copy\"",
		"watch:tsc": "tsc --watch",
		"watch:tailwind": "npx tailwindcss build src/tailwind-source.css -o src/tailwind.min.css --watch",
		"watch:copy": "chokidar 'src/tailwind.min.css' -c 'node lib-utils/copy-tailwind.js'"
	},
